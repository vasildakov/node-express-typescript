{
  "name": "docker_web_app",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "author": "Vasil Dakov <vasildakov@gmail.com>",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "test": "jest --coverage --verbose --testPathPattern=./tests"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**"
    ],
    "coverageDirectory": "./dist/build/coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest-mock/express": "^2.0.1",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.13",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.12",
    "babel-jest": "^29.3.1",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "exmpl": "^3.0.0",
    "jest": "^29.3.1",
    "node-dev": "^7.4.3",
    "node-mocks-http": "^1.12.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "node",
    "express",
    "javascript",
    "typescript",
    "docker"
  ],
  "license": "ISC"
}
